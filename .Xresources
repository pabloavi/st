!Import colors from .Xcolors
#include ".Xcolors"

*.dpi: 112
!Espaciado de fuente
URxvt.letterSpace: 0
URxvt.lineSpace: 0

!General
URxvt.saveline: 2048
URxvt.urgentOnBell: true
URxvt.iso14755: false
URxvt.iso14755_52: false

!Apariencia
URxvt.geometry: 92x24
URxvt.internalBorder: 0
URxvt.externalborder: 0
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.depth: 12 

!Keybindings
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.keysym.Shift-Control-V: eval:paste_clipboard


!Fuente
!URxvt*font: xft:Source Code Pro:pixelsize=16:foundry=adobe:width=normal:spacing=100:scalable=true
!URxvt*boldFont: xft:Source Code Pro:bold:pixelsize=16:foundry=adobe:width=normal:spacing=100:scalable=true

! URxvt*font: xft:SauceCodePro Nerd Font Mono:size=12
! URxvt*boldFont: xft:SauceCodePro Nerd Font Mono:bold:size=12
URxvt*font: xft:JetBrainsMono Nerd Font:style=regular:pixelsize=24
URxvt*boldFont: xft:JetBrainsMono Nerd Font:style=bold:pixelsize=24

!dmenu
dmenu.selforeground: #D8DEE9
dmenu.background: #2E3440
dmenu.selbackground: #BF616A
dmenu.foreground: #ECEFF4
